####
This is a basic jquery plugin that will allow you to pull in an instagram feed.
####

####
USAGE
####

--- getMyStream

<div id="me" />

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js"></script>
<script src="javascripts/jquery.instagram.js"></script>

$(document).ready(function() {
	$("#me").instagram('getMyStream', {
		count: "12"
	});
});


--- getUserStream

<div id="user" />

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js"></script>
<script src="javascripts/jquery.instagram.js"></script>

$(document).ready(function() {
	$("#user").instagram('getUserStream', {
		count: "12",
		user: 1574083
	});
});


--- getMyLikes

<div id="myLikes" />

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js"></script>
<script src="javascripts/jquery.instagram.js"></script>

$(document).ready(function() {
	$("#myLikes").instagram('getMyLikes', {
		count: "12"
	});
});